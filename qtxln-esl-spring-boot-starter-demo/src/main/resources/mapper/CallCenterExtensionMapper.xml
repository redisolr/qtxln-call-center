<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qtxln.mapper.CallCenterExtensionMapper">

  <select id="queryAll" resultType="com.qtxln.model.entity.CallCenterExtension">
    SELECT
      id,
      extension_name,
      extension_number
    FROM
      `call_center_extension`;
  </select>

</mapper>